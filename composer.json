{
    "name": "silverstripe/sharedraftcontent",
    "type": "silverstripe-vendormodule",
    "description": "Share draft page content with non-CMS users",
    "license": "BSD-3-Clause",
    "keywords": [
        "silverstripe"
    ],
    "authors": [
        {
            "name": "Damian Mooyman",
            "email": "damian.mooyman@gmail.com"
        },
        {
            "name": "Christopher Pitt",
            "email": "chris@silverstripe.com"
        },
        {
            "name": "David Craig",
            "email": "david@silverstripe.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "silverstripe/framework": "^5",
        "silverstripe/cms": "^5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3"
    },
    "extra": {
        "expose": [
            "client/dist",
            "client/lang"
        ]
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\ShareDraftContent\\": "src/",
            "SilverStripe\\ShareDraftContent\\Tests\\": "tests/",
            "SilverStripe\\ShareDraftContent\\Tests\\Behat\\Context\\": "tests/behat/src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
